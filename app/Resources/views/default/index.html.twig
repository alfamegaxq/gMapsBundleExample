{% extends 'base.html.twig' %}

{% block stylesheets %}
    <link href="{{ asset('libs/jquery-ui.css') }}" rel="stylesheet">
{% endblock %}


{% block body %}
    {{ render(controller('GMapsBundle:Suggest:renderCitySuggest', {
        inputParams: 'name="text" class="city_suggest"',
        language: 'lt',
        country: 'lt'
    })) }}
{% endblock %}

{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="{{ asset('libs/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('bundles/gmaps/js/script.js') }}"></script>
{% endblock %}